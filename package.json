{
  "name": "knowclip",
  "productName": "Knowclip",
  "version": "0.12.0-beta",
  "private": true,
  "engines": {
    "node": "^16.17.0"
  },
  "author": {
    "name": "Justin Silvestre",
    "email": "justinsilvestre@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knowclip/knowclip.git"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@material-ui/icons": "^4.5.1",
    "@mui/material": "^5.12.3",
    "@mui/styles": "^5.12.3",
    "@sentry/electron": "^2.5.4",
    "@silvestre/mkanki": "^3.4.0",
    "ajv": "^8.8.2",
    "asar": "3.1.0",
    "better-ajv-errors": "^1.2.0",
    "better-sqlite3": "^8.3.0",
    "clipwave": "^0.1.9",
    "dexie": "^3.0.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-store": "^8.0.1",
    "ffmpeg-static": "^4.4.0",
    "ffprobe-static": "^3.0.0",
    "filenamify": "^4.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.0.0",
    "@silvestre/material-ui-chip-input-v5": "^1.0.0",
    "moment": "^2.24.0",
    "normalizr": "^3.5.0",
    "papaparse": "^5.2.0",
    "pinyin-utils": "^2.32.0",
    "react": "^17.0.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-virtualized": "^9.22.5",
    "redux": "^4.1.2",
    "redux-observable": "^2.0.0",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0",
    "reselect": "^4.1.2",
    "rxjs": "^7.0.0",
    "semver": "^7.1.2",
    "subsrt": "^1.0.0",
    "subtitle": "^4.1.1",
    "tempy": "^1.0.0",
    "ts-json-schema-generator": "^0.97",
    "uuid": "^8.3.2",
    "wanakana": "^4.0.2",
    "webdriverio": "^8.0.0",
    "xss": "^1.0.6",
    "yaml": "^1.7.2",
    "yauzl": "^2.10.0"
  },
  "main": "./build-main/electron/main.js",
  "homepage": "./",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "react-scripts start",
    "build:homepage": "npm run -- --cwd homepage run build",
    "start:integration": "REACT_APP_CHROMEDRIVER=true npm run start",
    "start:integration-win": "set REACT_APP_CHROMEDRIVER=true&& npm run start",
    "build-main": "npm run tsc -- ./electron/main.ts --outDir ./build-main",
    "webpack": "npm run build-schemas && REACT_APP_BUILD_NUMBER=$GITHUB_RUN_NUMBER react-scripts build",
    "webpack:win": "react-scripts build",
    "webpack:win-COMMENT": "# skipping schemas build because of backslashes in script",
    "webpack:ci": "CI=false npm run webpack",
    "webpack:ci-win": "set CI=false&& npm run webpack:win",
    "webpack:appveyor": "react-scripts build",
    "test": "npm run build-main && npm run vitest run -- --config ./vitest.config.ts",
    "integration": "npm run build-main && REACT_APP_CHROMEDRIVER=true npm run webpack:ci && npm run vitest run -- --config ./vitest-integration.config.ts",
    "integration:win": "set REACT_APP_CHROMEDRIVER=true&& npm run build-main && npm run webpack:ci-win && npm run vitest run -- --config ./vitest-integration.config.ts",
    "integration:skip-build": "npm run build-main && REACT_APP_CHROMEDRIVER=true npm run vitest run -- --config ./vitest-integration.config.ts",
    "integration:skip-build-win": "npm run build-main && set REACT_APP_CHROMEDRIVER=true&& npm run vitest run -- --config ./vitest-integration.config.ts",
    "integration:debug": "REACT_APP_CHROMEDRIVER=true INTEGRATION_DEV=true npm run integration:skip-build -- --verbose",
    "integration:debug-win": "set REACT_APP_CHROMEDRIVER=true&&set INTEGRATION_DEV=true&& npm run integration:skip-build-win -- --verbose",
    "build-fixture:saved": "npm run integration:build-fixture -- -t \"savedProject\"",
    "build-fixture:shared": "npm run integration:build-fixture -- -t \"sharedProject\" && rm -rf src/test/assets/generated/sharedProject sharedProject_persistedDataSnapshot.js && echo 'removed generated files'",
    "build-fixture:subtitles": "npm run integration:build-fixture -- -t \"subtitlesProject\"",
    "integration:build-fixture": "npm run build-main && INTEGRATION_DEV=true REACT_APP_CHROMEDRIVER=true BUILDING_FIXTURES=true npm run vitest run -- --config ./vitest-integration-fixtures.config.ts",
    "eject": "react-scripts eject",
    "electron": "export NODE_ENV=development && npm run build-main && electron .",
    "electron:win": "set NODE_ENV=development&& npm run build-main && electron .",
    "lint": "npm run eslint 'src/**/*.{ts,tsx,js}'",
    "vitest": "vitest",
    "lint-staged": "lint-staged",
    "eslint": "eslint",
    "prettier": "prettier",
    "format": "prettier --ignore-path ./.gitignore \"{src/**,.}/*.{js,ts,tsx}\"",
    "format:write": "npm run format -- --write",
    "tsc": "tsc",
    "build-schemas": "npm run build-schema:project && npm run build-schema:media",
    "build-schema:project": "ts-json-schema-generator --path ./src/types/Project.ts --tsconfig ./tsconfig.json --out ./src/utils/validateProjectMetadata.json --type \\ProjectMetadataJson",
    "build-schema:media": "ts-json-schema-generator --path ./src/types/Project.ts --tsconfig ./tsconfig.json --out ./src/utils/validateProjectMedia.json --type '$MediaJson'",
    "tidy": "npm run format -- --list-different && npm run lint && npm run tsc",
    "electron-builder": "electron-builder",
    "electron-builder:skip-publish": "electron-builder -p never",
    "dist": "npm run build-main && npm run webpack:ci && npm run electron-builder:skip-publish",
    "dist:appveyor": "npm run build-main && npm run webpack:appveyor && npm run electron-builder",
    "yalc": "yalc",
    "husky": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "npm run prettier -- --write --ignore-path ./.gitignore",
      "npm run eslint"
    ]
  },
  "browserslist": [
    "electron >= 14.2.0"
  ],
  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/better-sqlite3": "^7.4.0",
    "@types/chromedriver": "^81.0.0",
    "@types/fluent-ffmpeg": "^2.1.19",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.17.27",
    "@types/papaparse": "^5.0.1",
    "@types/react": "^17.0.31",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "^9.21.14",
    "@types/request": "^2.48.5",
    "@types/semver": "^7.3.2",
    "@types/subtitle": "^2.0.2",
    "@types/uuid": "^8.3.1",
    "@types/wanakana": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chromedriver": "^108.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^7.0.4",
    "jsdom": "^22.0.0",
    "levenary": "^1.1.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.1.2",
    "request": "^2.88.2",
    "silvestre-afa-react-scripts": "5.0.1-5",
    "typescript": "^4.9.4",
    "vitest": "^0.31.0",
    "yalc": "^1.0.0-pre.53"
  },
  "resolutions": {
    "@types/react": "^17.0.31"
  },
  "build": {
    "appId": "com.electron.knowclip",
    "asar": true,
    "productName": "Knowclip",
    "npmRebuild": true,
    "copyright": "Copyright 2020 Justin Silvestre",
    "files": [
      "./build/**/*",
      "./icons/**/*",
      "./build-main/**/*"
    ],
    "win": {
      "target": [
        "portable"
      ],
      "icon": "./icons/icon.ico",
      "artifactName": "${productName}_${version}_${os}_${arch}.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Education",
      "artifactName": "${productName}_${version}_${os}.${ext}"
    },
    "mac": {
      "target": [
        "default"
      ],
      "identity": null,
      "category": "public.app-category.education",
      "icon": "./icons/icon.icns",
      "artifactName": "${productName}_${version}_${os}.${ext}"
    }
  }
}
