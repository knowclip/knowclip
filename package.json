{
  "name": "knowclip",
  "productName": "Knowclip",
  "version": "0.11.0-beta",
  "private": true,
  "engines": {
    "node": "^14.18.0"
  },
  "author": {
    "name": "Justin Silvestre",
    "email": "justinsilvestre@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knowclip/knowclip.git"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@sentry/electron": "^2.5.4",
    "@silvestre/mkanki": "^3.3.0",
    "ajv": "^8.6.3",
    "asar": "3.1.0",
    "better-ajv-errors": "^0.7.0",
    "better-sqlite3": "^7.4.4",
    "clipwave": "^0.1.8",
    "dexie": "^3.0.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-store": "^8.0.1",
    "ffmpeg-static": "^4.4.0",
    "ffprobe-static": "^3.0.0",
    "filenamify": "^4.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.0.0",
    "material-ui-chip-input": "^1.1.0",
    "moment": "^2.24.0",
    "normalizr": "^3.5.0",
    "papaparse": "^5.2.0",
    "pinyin-utils": "^2.32.0",
    "react": "^17.0.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-virtualized": "^9.22.3",
    "redux": "^4.1.1",
    "redux-observable": "^2.0.0",
    "redux-persist": "^6.0.0",
    "redux-persist-electron-storage": "^2.1.0",
    "reselect": "^4.0.0",
    "rxjs": "^7.0.0",
    "semver": "^7.1.2",
    "subsrt": "^1.0.0",
    "subtitle": "^4.1.1",
    "tempy": "^1.0.0",
    "ts-json-schema-generator": "^0.96",
    "uuid": "^8.3.2",
    "wanakana": "^4.0.2",
    "webdriverio": "^7.16.3",
    "xss": "^1.0.6",
    "yaml": "^1.7.2",
    "yauzl": "^2.10.0"
  },
  "main": "./build-main/electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build:homepage": "yarn --cwd homepage run build",
    "postinstall": "electron-builder install-app-deps",
    "start:integration": "REACT_APP_CHROMEDRIVER=true yarn start",
    "start:integration-win": "set REACT_APP_CHROMEDRIVER=true&& yarn start",
    "build-main": "yarn tsc ./electron/main.ts --outDir ./build-main",
    "webpack": "yarn build-schemas && REACT_APP_BUILD_NUMBER=$GITHUB_RUN_NUMBER react-scripts build",
    "webpack:win": "react-scripts build",
    "webpack:win-COMMENT": "# skipping schemas build because of backslashes in script",
    "webpack:ci": "CI=false yarn webpack",
    "webpack:ci-win": "set CI=false&& yarn webpack:win",
    "webpack:appveyor": "react-scripts build",
    "test": "yarn build-main && react-scripts test --env=jsdom ./src --testPathIgnorePatterns=./src/test",
    "integration": "yarn build-main && REACT_APP_CHROMEDRIVER=true yarn webpack:ci && react-scripts test ./src/test/ --watchAll=false  --runInBand",
    "integration:win": "set REACT_APP_CHROMEDRIVER=true&& yarn build-main && yarn webpack:ci-win && react-scripts test ./src/test/ --watchAll=false  --runInBand",
    "integration:skip-build": "yarn build-main && REACT_APP_CHROMEDRIVER=true react-scripts test ./src/test/ --watchAll=false --runInBand",
    "integration:skip-build-win": "yarn build-main && set REACT_APP_CHROMEDRIVER=true&& react-scripts test ./src/test/ --watchAll=false --runInBand",
    "integration:debug": "INTEGRATION_DEV=true yarn integration:skip-build --verbose",
    "integration:debug-win": "set INTEGRATION_DEV=true&& yarn integration:skip-build-win --verbose",
    "build-fixture:saved": "yarn integration:build-fixture -t \"testing saved projects\"",
    "build-fixture:shared": "yarn integration:build-fixture -t \"shared project\" && rm -rf src/test/assets/generated/sharedProject sharedProject_persistedDataSnapshot.js && echo 'removed generated files'",
    "build-fixture:subtitles": "yarn integration:build-fixture -t \"from subtitles\"",
    "integration:build-fixture": "yarn build-main && INTEGRATION_DEV=true REACT_APP_CHROMEDRIVER=true BUILDING_FIXTURES=true react-scripts test ./src/test/ --testMatch=**/*.fixture.{js,jsx,ts,tsx} --watchAll=false --runInBand",
    "eject": "react-scripts eject",
    "electron": "export NODE_ENV=development && yarn build-main && electron .",
    "electron:win": "set NODE_ENV=development&& yarn build-main && electron .",
    "lint": "eslint 'src/**/*.{ts,tsx,js}'",
    "prettier": "prettier",
    "format": "prettier --ignore-path ./.gitignore \"{src/**,.}/*.{js,ts,tsx}\"",
    "format:write": "yarn format --write",
    "tsc": "tsc",
    "build-schemas": "yarn build-schema:project && yarn build-schema:media",
    "build-schema:project": "yarn build-schema --out ./src/utils/validateProjectMetadata.json --type \\ProjectMetadataJson ",
    "build-schema:media": "yarn build-schema --out ./src/utils/validateProjectMedia.json --type \\$MediaJson",
    "build-schema": "ts-json-schema-generator --path ./src/types/Project.ts --tsconfig ./tsconfig.json",
    "tidy": "yarn format --list-different && yarn lint && yarn tsc",
    "electron-builder": "electron-builder",
    "electron-builder:skip-publish": "electron-builder -p never",
    "dist": "yarn build-main && yarn webpack:ci && yarn electron-builder:skip-publish",
    "dist:appveyor": "yarn build-main && yarn webpack:appveyor && yarn electron-builder",
    "yalc": "yalc",
    "husky": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "yarn prettier --write --ignore-path ./.gitignore",
      "yarn eslint"
    ]
  },
  "browserslist": [
    "electron >= 14.2.0"
  ],
  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/better-sqlite3": "^7.4.0",
    "@types/chromedriver": "^81.0.0",
    "@types/classnames": "^2.2.9",
    "@types/fluent-ffmpeg": "^2.1.18",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.17.27",
    "@types/papaparse": "^5.0.1",
    "@types/react": "^17.0.31",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "^9.21.14",
    "@types/request": "^2.48.5",
    "@types/reselect": "^2.2.0",
    "@types/semver": "^7.3.2",
    "@types/subtitle": "^2.0.2",
    "@types/uuid": "^8.3.1",
    "@types/wanakana": "^4.0.3",
    "@types/yaml": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.1.0",
    "chromedriver": "^93.0.0",
    "electron": "^14.0.0",
    "electron-builder": "^22.13.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "levenary": "^1.1.1",
    "lint-staged": "^11.2.3",
    "prettier": "^2.1.2",
    "request": "^2.88.2",
    "silvestre-afa-react-scripts": "^4.0.3",
    "typescript": "^4.4.4",
    "yalc": "^1.0.0-pre.53"
  },
  "resolutions": {
    "@types/react": "^17.0.31"
  },
  "build": {
    "appId": "com.electron.knowclip",
    "asar": true,
    "productName": "Knowclip",
    "copyright": "Copyright 2020 Justin Silvestre",
    "files": [
      "./build/**/*",
      "./icons/**/*",
      "./build-main/**/*"
    ],
    "win": {
      "target": [
        "portable"
      ],
      "icon": "./icons/icon.ico",
      "artifactName": "${productName}_${version}_${os}_${arch}.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Education",
      "artifactName": "${productName}_${version}_${os}.${ext}"
    },
    "mac": {
      "target": [
        "default"
      ],
      "identity": null,
      "category": "public.app-category.education",
      "icon": "./icons/icon.icns",
      "artifactName": "${productName}_${version}_${os}.${ext}"
    }
  }
}
